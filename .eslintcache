\n                </form>\n            </div>\n        </nav>\n\n\n        // <nav className=\"navbar navbar-dark bg-dark justify-content-between\">\n        //     <a className=\"navbar-brand\">Catálogo</a>\n        //     <button className=\"navbar-toggler\" type=\"button\" data-toggle=\"collapse\" data-target=\"#navbarNavAltMarkup\" aria-controls=\"navbarNavAltMarkup\" aria-expanded=\"false\" aria-label=\"Toggle navigation\">\n        //         <span className=\"navbar-toggler-icon\"></span>\n        //     </button>\n\n        //     <div className=\"collapse navbar-collapse\" id=\"navbarNavAltMarkup\">\n\n        //         <div className=\"navbar-nav\">\n        //             <a className=\"nav-item nav-link active\" href=\"/\">Home <span className=\"sr-only\">(current)</span></a>\n        //             <a className=\"nav-item nav-link\" href=\"/crud\">Crud</a>\n        //         </div>\n        //         <form className=\"form-inline\" onSubmit={handledSubmit}>\n        //             <input className=\"form-control mr-sm-2\" type=\"search\" placeholder=\"Search\" aria-label=\"Search\" value={inputValue} onChange={handledInputChange} />\n        //             <button className=\"btn btn-outline-success my-2 my-sm-0\" type=\"submit\" onChange={handledInputChange}>Search</button>\n        //         </form>\n        //     </div>\n\n        // </nav>\n\n\n\n    )\n}\n\nMyNavbar.propTypes = {\n    setSearch: PropTypes.func.isRequired\n}\n","/Users/mauriciocarrenogaete/Desktop/React/desafio-ripley-front/src/components/product/HomeScreen.js",[],"/Users/mauriciocarrenogaete/Desktop/React/desafio-ripley-front/src/hooks/useFetchProducts.js",["64","65"],"/Users/mauriciocarrenogaete/Desktop/React/desafio-ripley-front/src/helpers/Products.js",[],"/Users/mauriciocarrenogaete/Desktop/React/desafio-ripley-front/src/ProductGridItem.js",[],"/Users/mauriciocarrenogaete/Desktop/React/desafio-ripley-front/src/components/product/CrudScreen.js",["66"],"/Users/mauriciocarrenogaete/Desktop/React/desafio-ripley-front/src/components/product/ProductTable.js",["67"],"import React, { useState } from 'react'\n\nexport const ProductTable = (props) => {\n\n    console.log(props.results)\n\n\n    return (\n        <table>\n            <thead>\n                <tr>\n                    <th>ID</th>\n                    <th>Nombre</th>\n                    <th>Marca</th>\n                    <th>Descripción</th>\n                    <th>Precio</th>\n\n                    <th>Acciones</th>\n                </tr>\n            </thead>\n            <tbody>\n                {\n                    props.results.length > 0 ?\n                        props.results.map(product => (\n                            <tr key={product.id}>\n                                <td>{product.id}</td>\n                                <td>{product.nombre}</td>\n                                <td>{product.marca}</td>\n                                <td>{product.descripcion}</td>\n                                <td>{product.precio}</td>\n                                <td>\n                                    <button className=\"button muted-button\" onClick={() => {props.editRow(product)}}>Editar</button>&nbsp;\n                                <button className=\"button muted-button\" onClick={() => {props.deleteProduct(product.id)}}>Eliminar</button>\n                                </td>\n                            </tr>\n                        )) : (<tr>\n                            <td key={6} colSpan={6}>No Existen Productos</td>\n                        </tr>\n                        )\n                }\n\n            </tbody>\n        </table>\n    )\n}\n\nexport default ProductTable","/Users/mauriciocarrenogaete/Desktop/React/desafio-ripley-front/src/components/product/AddProductForm.js",[],"/Users/mauriciocarrenogaete/Desktop/React/desafio-ripley-front/src/components/product/EditProductForm.js",["68"],"\nimport React from 'react'\nimport { useForm } from 'react-hook-form';\nimport { UseFetchInsertProducts } from '../../hooks/useFetchProducts'\n\nexport const EditProductForm = (props) => {\n\n    //console.log(props.currentProduct)\n    const { register, errors, handleSubmit, setValue } = useForm({\n        defaultValues: props.currentProduct\n    });\n\n    setValue('id', props.currentProduct.id);\n    setValue('nombre', props.currentProduct.nombre);\n    setValue('marca', props.currentProduct.marca);\n    setValue('descripcion', props.currentProduct.descripcion);\n    setValue('precio', props.currentProduct.precio);\n    setValue('imagen', props.currentProduct.imagen);\n\n    const onSubmit = (data, e) => {\n        console.log(data);\n        data.id = props.currentProduct.id;\n        props.updateProduct(props.currentProduct.id, data)\n        e.target.reset();\n    }\n\n    return (\n        <form onSubmit={handleSubmit(onSubmit)}>\n            <label>ID</label>\n            <input type=\"text\" name=\"id\" ref={\n                register({\n                    required: {\n                        value: true, message: 'Campo Obligatorio'\n                    }\n                })\n            } />\n            <div>{errors?.id?.message}</div>\n            <label>Nombre</label>\n            <input type=\"text\" name=\"nombre\" ref={\n                register({\n                    required: {\n                        value: true, message: 'Campo Obligatorio'\n                    }\n                })\n            } />\n            <div>{errors?.nombre?.message}</div>\n            <label>Marca</label>\n            <input type=\"text\" name=\"marca\" ref={\n                register({\n                    required: {\n                        value: true, message: 'Campo Obligatorio'\n                    }\n                })\n            } />\n            <div>{errors?.marca?.message}</div>\n            <label>Descripción</label>\n            <input type=\"text\" name=\"descripcion\" ref={\n                register({\